{
	"rules":
	{
		"class-name": true,
		"comment-format": [
			true,
			"check-space"
		],
		"eofline": true,
		"indent": [
			true,
			"tabs"
		],
		"quotemark": [
			true,
			"double",
			"avoid-escape"
		],
		"no-duplicate-variable": true,
		"no-eval": true,
		"no-internal-module": true,
		"no-trailing-whitespace": true,
		"no-unsafe-finally": true,
		"no-var-keyword": true,
		"one-line": [
			true,
			"check-whitespace"
		],
		"semicolon": [
			true,
			"always"
		],
		"triple-equals": [
			true,
			"allow-null-check"
		],
		"typedef-whitespace": [
			true,
			{
				"call-signature": "nospace",
				"index-signature": "nospace",
				"parameter": "nospace",
				"property-declaration": "nospace"
			}
		],
		"variable-name": [
			true,
			"ban-keywords"
		],
		"whitespace": [
			true,
			"check-decl",
			"check-operator",
			"check-separator",
			"check-type"
		],
		"prefer-const": true
	}
}
